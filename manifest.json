{
    "name": "When2Meet Filler",
    "description": "An extension that allows users to fill in when2meet with ics files",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
      "thumbnail": "when2meet-icon-filled-256.png"
    },

    "action":{
        "default_popup": "index.html",
        "default_title": "When2Meet Filler"
    },

    "content_scripts": [
        {
          "js": ["editor.js"],
          "matches": ["https://www.when2meet.com/*"],
          "type": "module"
        }
    ],

    "permissions": [
      "activeTab"]

}